{
    "family": "blue-green-task-def",
    "containerDefinitions": [
        {
            "name": "redmine",
            "image": "<IMAGE1_NAME>",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "redmine-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "MYSQL_PASSWORD",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:951053526398:parameter/ecs/redmine/mysql/database-password"
                },
                {
                    "name": "MYSQL_DB",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:951053526398:parameter/ecs/redmine/mysql/database-name"
                },
                {
                    "name": "MYSQL_HOST",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:951053526398:parameter/ecs/redmine/mysql/database-host"
                },
                {
                    "name": "MYSQL_USER_NAME",
                    "valueFrom": "arn:aws:ssm:ap-southeast-1:951053526398:parameter/ecs/redmine/mysql/database-username"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/redmine/logs",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::951053526398:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::951053526398:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "1024",
    "ephemeralStorage": {
        "sizeInGiB": 21
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
